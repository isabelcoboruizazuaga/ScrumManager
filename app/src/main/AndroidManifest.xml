<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.scrummanager">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/logobig"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/logobig"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.Light.NoActionBar">
        <activity android:name=".Vistas.NuevaTareaActivity"></activity>
        <activity android:name=".Vistas.EditarSprintActivity" />
        <activity android:name=".Vistas.NuevoSprintActivity" />
        <activity android:name=".Vistas.EditarProyectoActivity" />
        <activity android:name=".Vistas.NuevoProyectoActivity" />
        <activity android:name=".Vistas.VerClienteActivity" />
        <activity android:name=".Vistas.EditarClienteActivity" />
        <activity android:name=".Vistas.NuevoClienteActivity" />
        <activity android:name=".Vistas.VerDepartamentoActivity" />
        <activity android:name=".Vistas.EditarDepartamentoActivity" />
        <activity android:name=".Vistas.NuevoDepartamentoActivity" />
        <activity android:name=".Vistas.VerEmpleadoActivity" />
        <activity android:name=".Vistas.EditarEmpleadoActivity" />
        <activity android:name=".Vistas.NuevoEmpleadoActivity" />
        <activity
            android:name=".Vistas.LoginActivity"
            android:label="@string/app_name"
            android:theme="@style/Theme.ScrumManager.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".NavigationDrawerActivity"
            android:label="@string/app_name"
            android:theme="@style/Theme.ScrumManager.NoActionBar" />
    </application>

</manifest>